<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--
    FFXIV クライアント:ffxiv_dx11
    Google Chrome ブラウザ:chrome
    メモ帳:notepad
    電卓:Calculator
    FlowScape:FlowScape
    -->
    <add key="PROCESS_NAME" value="ffxiv_dx11" />

    <add key="SAVE.DIRECTORY" value="" /><!-- Default:MyPictures\%PROCESS_NAME%_CAPTURE\ -->
    <add key="SAVE.FILE.DIRECTORY" value="\YYYY\MM\DD" />
    <add key="SAVE.FILE.NAME" value="IMG_\INDEX" />
    <add key="SAVE.FILE.TYPE" value="PNG+JPEG" /><!-- PNG,JPEG,TIFF,PNG+JPEG,TIFF+JPEG -->
    <add key="SAVE.FILE.QUALITY" value="100" />
    <add key="SAVE.FILE.COPYRIGHT" value="0" /><!-- Enabled First Capture File -->
    <add key="SAVE.FILE.INDEX" value="1" />
    
    <add key="GUIDE.LOCATION.ALIGNMENT" value="Right+Top" /><!-- Center,Left+Top,Left+Bottom,Right+Top,Right+Bottom -->
    
    <add key="TRIM.LOCATION.LEFT" value="0" /><!-- LEFT = 0 And TOP = 0 Then Defaut Location -->
    <add key="TRIM.LOCATION.TOP" value="0" />
    <add key="TRIM.SIZE.WIDTH" value="600" />
    <add key="TRIM.SIZE.HEIGHT" value="400" />
    <add key="TRIM.ASPECT_RATIO.TYPE" value="3" />
    <add key="TRIM.COMPOTION.TYPE" value="2" />
  </appSettings>
</configuration>