<Window x:Class="WPF_SAVE_CONFIRM"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GPOSE_ASSIST"
        mc:Ignorable="d"
        Title="Save confirm" Height="450" Width="800" MinWidth="800" MinHeight="450">
    <Grid>
        <Image x:Name="PCB_IMAGE"/>
        <Grid VerticalAlignment="Bottom" Margin="5,5,5,5">
            <Button x:Name="BTN_ROTATE_R" HorizontalAlignment="Left" Margin="5,5,5,5" Height="30" Width="80" Content="Rotate"/>
            <CheckBox x:Name="CHK_COPYRIGHT" Content="Copyright" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="90,5,5,5"/>
            <Button x:Name="BTN_OK" HorizontalAlignment="Right" Margin="5,5,90,5" Height="30" Width="80" Content="OK"/>
            <Button x:Name="BTN_CANCEL" HorizontalAlignment="Right" Margin="5,5,5,5" Height="30" Width="80" Content="Cancel"/>
        </Grid>
    </Grid>
</Window>
